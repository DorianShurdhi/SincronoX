<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletController</servlet-name>
  	<display-name>ServletController</display-name>
  	<description></description>
  	<servlet-class>com.supernalotto.controller.ServletController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NuovaServlet</servlet-name>
  	<display-name>NuovaServlet</display-name>
  	<description></description>
  	<servlet-class>com.superenalotto.controller.NuovaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletController</servlet-name>
  	<url-pattern>/ServletController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NuovaServlet</servlet-name>
  	<url-pattern>/NuovaServlet</url-pattern>
  </servlet-mapping>
  <resource-ref>
      <description>DB Connection</description>
      <res-ref-name>jdbc/superenalotto</res-ref-name>
      <res-type>javax.sql.DataSource</res-type>
      <res-auth>Container</res-auth>
  </resource-ref>
</web-app>
